{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@api/*": ["src/api/*"],

      "@adapters/*": ["src/adapters/*"],
      "@helpers/*": ["src/_helpers/*"],

      "@components/*": ["src/components/*"],
      "@generic/*": ["src/components/generic/*"],
      "@screens/*": ["src/components/screens/*"],
      "@static/*": ["src/components/static/*"],

      "@store/*": ["src/store/*"],
      "@event/*": ["src/event/*"],
      "@services/*": ["src/services/*"],

      "@icons/*": ["src/assets/icons/*"],
      "@styles/*": ["src/styles/*"],
      "@utils/*": ["src/utils/*"],

      "@skeletons": ["src/skeletons"],
      "@hooks": ["src/hooks"],

      "@newcomponents/*": ["src/newcomponents/*"],
      "@newstyles/*": ["src/newstyles/*"]
    }
  }
}
